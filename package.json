{
  "name": "product_previews",
  "version": "1.0.0",
  "description": "customer product reviews ",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --watch",
    "test": "jest",
    "start": "npx nodemon server/index.js",
    "seed": "node database/seed.js"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "elias",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "ajax": "0.0.4",
    "ajax-request": "^1.2.3",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "mongo": "^0.1.0",
    "mongoose": "^5.9.4",
    "mongoose-data-seed": "^2.1.6",
    "nodemon": "^2.0.2",
    "react": "^16.13.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^16.13.0",
    "react-dropdown": "^1.7.0",
    "react-moment": "^0.9.7",
    "react-paginate": "^6.3.2",
    "react-star-ratings": "^2.3.0",
    "style-loader": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Etsadik/product_reviews.git"
  },
  "bugs": {
    "url": "https://github.com/Etsadik/product_reviews/issues"
  },
  "homepage": "https://github.com/Etsadik/product_reviews#readme"
}
